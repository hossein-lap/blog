security:
  exec:
    allow: ['^dart-sass-embedded$', '^go$', '^npx$', '^postcss$', '^asciidoctor$']


markup:
  goldmark:
    renderer:
      unsafe: true
  highlight:
    noClasses: true
    lineNos: true
    lineNumberInTable: false
    anchorLineNos: false
    lineAnchors: ""
    # style: gruvbox
    codeFences: true
    guessSyntax: false
    hl_Lines: ""
    hl_inline: false
    lineNoStart: 1
    noHl: false
    tabWidth: 8

